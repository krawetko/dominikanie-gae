<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <display-name>welcomeServlet</display-name>
        <servlet-name>WelcomeServlet</servlet-name>
        <servlet-class>com.krawetko.dominikanie.WelcomeServlet</servlet-class>
    </servlet>
    <servlet>
        <display-name>searchForOpenMeetings</display-name>
        <servlet-name>SearchServlet</servlet-name>
        <servlet-class>com.krawetko.dominikanie.SearchForOpenMeetingsServlet</servlet-class>
    </servlet>
    <servlet>
        <display-name>dailyReport</display-name>
        <servlet-name>DailyReport</servlet-name>
        <servlet-class>com.krawetko.dominikanie.DailyRaport</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>WelcomeServlet</servlet-name>
        <url-pattern>/*</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SearchServlet</servlet-name>
        <url-pattern>/search</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DailyReport</servlet-name>
        <url-pattern>/report</url-pattern>
    </servlet-mapping>

</web-app>
